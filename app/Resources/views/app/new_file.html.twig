{% extends 'base.html.twig' %}
{% block title %}Dodaj nowy plik CSV{% endblock %}
{% block body %}
    <div class="row">


    <div class="col-md-offset-3 col-md-6">

        <div class="text-center">

            {{ form_start(form) }}

            <div class="row">
                <div class="col-md-offset-3 col-md-6">
                    {{ form_label(form.file) }}
                    {{ form_widget(form.file) }}
                </div>
            </div>


            <button class="btn btn-primary" type="submit"> DODAJ</button>
            {{ form_end(form) }}
            <a class="btn btn-primary" href="{{ path('homepage') }}">Wróć do wykresu </a>
        </div>
    </div>
    </div>
{% endblock %}


